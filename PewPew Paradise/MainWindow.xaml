<Window x:Class="PewPew_Paradise.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PewPew_Paradise"
        mc:Ignorable="d"
        Title="MainWindow" Height="512" Width="512" KeyDown="KeyPress" KeyUp="KeyLift" SizeToContent="WidthAndHeight" MinWidth="200" MinHeight="220" Background="{DynamicResource BackgroundBrush}">

    <Border x:Name="GameWindow" BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="512" Margin="0,0,0,0" VerticalAlignment="Top" Width="512">
        <Label x:Name="UwU" Content="UwU" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="80"/>
    </Border>

</Window>
